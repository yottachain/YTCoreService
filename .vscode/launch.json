{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch MakeCode",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}/main_test.go",
            "args": ["makeconst"]
