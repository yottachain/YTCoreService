{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch MakeCode",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}/main_test.go",
            "args": ["makeconst"]
        },
        {
            "name": "Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}/main_test.go",
            "env":{
                "YTFS_HOME":"D:/YTCoreService"
            },
            "args": ["s3"]
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}",
            "env":{
                "ProgramName":"YTS3",
                "YTSN_HOME":"D:/YTCoreService",
                "YTFS_HOME":"D:/YTCoreService"
            },
            "args": ["test","console"]
        }
    ]
}